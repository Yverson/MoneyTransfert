<body class="login-container login-cover">
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">
            <!-- Main content -->
            <div class="content-wrapper">

                <div class="panel panel-body login-form">
                    <form asp-controller="Account" asp-action="Register" method="post" class="form-horizontal" role="form">
                        <div class="text-center">
                            <div class="icon-object border-success text-success"><i class="icon-plus3"></i></div>
                            <h5 class="content-group">Create new account <small class="display-block">All fields are required</small></h5>
                        </div>
                        <div asp-validation-summary="ValidationSummary.All" class="text-danger"></div>
                        <div class="form-group has-feedback has-feedback-left">
                            <input asp-for="Email" type="email" class="form-control" placeholder="Your username" required>
                            <div class="form-control-feedback">
                                <i class="icon-mention text-muted"></i>
                            </div>
                            <span asp-validation-for="Email" class="text-danger"></span>
                        </div>
                        <div class="form-group has-feedback has-feedback-left">
                            <input asp-for="Password" class="form-control" placeholder="Create password">
                            <div class="form-control-feedback">
                                <i class="icon-user-lock text-muted"></i>
                            </div>
                            <span asp-validation-for="Password" class="text-danger"></span>
                        </div>
                        <div class="form-group has-feedback has-feedback-left">
                            <input asp-for="ConfirmPassword" class="form-control" placeholder="Confirme password">
                            <div class="form-control-feedback">
                                <i class="icon-user-lock text-muted"></i>
                            </div>
                            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                        </div>
                        <div class="content-divider text-muted form-group"><span>Additions</span></div>
                        <div class="form-group">
                            <div class="checkbox">
                                <label>
                                    <input type="checkbox" class="styled">
                                    Accept <a href="#">terms of service</a>
                                </label>
                            </div>
                        </div>
                        <button type="submit" class="btn bg-indigo-400 btn-block">Register <i class="icon-circle-right2 position-right"></i></button>
                    </form>
                </div>
            </div>
            <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->
    <!-- Footer -->
    <div class="footer text-white text-center">
        &copy; 2015. <a href="#" class="text-white">Limitless Web App Kit</a> by <a href="http://themeforest.net/user/Kopyov" class="text-white" target="_blank">Eugene Kopyov</a>
    </div>
    <!-- /footer -->
    @await Html.PartialAsync("_ValidationScriptsPartial")
</body>
